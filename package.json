{
  "name": "express-api-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prestart": "babel ./src --out-dir build",
    "start": "node ./build/bin/www --es-module-specifier-resolution=node",
    "startdev": "nodemon --exec babel-node ./src/bin/www --es-module-specifier-resolution=node",
    "lint": "./node_modules/.bin/eslint ./src",
    "pretty": "prettier --write '**/*.{js,json}' '!node_modules/**'",
    "postpretty": "yarn lint --fix",
    "test": "nyc --reporter=html --reporter=text --reporter=lcov mocha -r @babel/register --es-module-specifier-resolution=node",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "runQuery": "babel-node ./src/utils/runQuery --es-module-specifier-resolution=node"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.17.9",
    "chai": "^4.3.6",
    "coveralls": "^3.1.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.2.2"
  }
}
